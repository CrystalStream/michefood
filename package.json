{
  "name": "michefood",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "lint": "standard",
    "test": "mocha ./tests --timeout 10000 -C --full-trace --exit",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "coverage:open": "nyc --reporter=lcov --reporter=text-lcov npm test && open ./coverage/lcov-report/index.html"
  },
  "dependencies": {
    "cookie-parser": "1.4.4",
    "debug": "2.6.9",
    "dotenv": "8.1.0",
    "express": "4.16.1",
    "mongoose": "5.6.11",
    "morgan": "1.9.1",
    "standard": "14.0.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.0",
    "mongodb-memory-server": "^5.2.0",
    "nodemon": "1.19.1",
    "nyc": "^14.1.1"
  },
  "nyc": {
    "exclude": [
      "**/test/**"
    ]
  },
  "standard": {
    "ignore": [
      "**/tests/"
    ]
  }
}
